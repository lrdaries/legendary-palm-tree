{
  "name": "divas-kloset",
  "version": "1.0.0",
  "description": "Diva's Kloset - E-commerce platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "prod": "NODE_ENV=production node server.js",
    "vercel-build": "cd frontend && npm install && npm run build",
    "migrate": "node migrate.js",
    "migrate-to-postgres": "node migrate-to-postgres.js",
    "setup-vercel": "node setup-vercel-env.js",
    "setup-admin": "node scripts/setup-admin.js",
    "pm2": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "audit": "npm audit && npx snyk test",
    "build-frontend": "cd frontend && npm install && npm run build",
    "dev:frontend": "cd frontend && npm run dev",
    "install-frontend": "cd frontend && npm install",
    "build-frontend-only": "cd frontend && npm run build"
  },
